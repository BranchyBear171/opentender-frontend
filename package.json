    "classlist.js": "1.1.20150312",
    "cookie-parser": "=1.4.3",
    "core-js": "=2.4.1",
    "css-loader": "^0.28.0",
    "d3-array": "^1.2.0",
    "d3-brush": "^1.0.4",
    "d3-force": "^1.0.6",
