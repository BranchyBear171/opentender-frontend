{
  "name": "opentender-portal-frontend",
  "version": "0.0.2",
  "description": "Opentender Portal Frontend (Server/Client)",
  "author": "okfde",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/digiwhist/opentender-frontend.git"
  },
  "scripts": {
    "watch": "webpack --watch --display-error-details",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "build:prod": "webpack --progress -p",
    "---prestart": "npm run build",
    "develop": "nodemon dist/server/index.js",
    "server": "node dist/server/index.js",
    "lint": "tslint \"src/**/*.ts\"",
    "schema": "cd bin && node schema2ts.js",
    "langs": "cd bin && node languages2files.js",
    "debug": "node --debug-brk dist/server/index.js",
    "docs": "node_modules/.bin/compodoc -p tsconfig.json -d ./dist/docs/",
    "serve-docs": "node_modules/.bin/compodoc -s -d ./dist/docs/",
    "typedoc-docs": "npm run typedoc -- --options typedoc.json --inlineSourceMap --exclude '**/*.spec.ts' ./src/",
    "typedoc": "typedoc",
    "treeshake": "node --max_old_space_size=8192 node_modules/webpack/bin/webpack.js --config webpack.treeshake.config.js --progress -p"
  },
  "dependencies": {
    "@angular/animations": "^4.3.1",
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/platform-server": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@angular/tsc-wrapped": "latest",
    "@asymmetrik/angular2-leaflet": "^2.1.5",
    "@types/body-parser": "^1.16.4",
    "@types/compression": "=0.0.33",
    "@types/cookie-parser": "^1.4.0",
    "@types/d3-brush": "^1.0.7",
    "@types/d3-shape": "^1.2.1",
    "@types/d3-transition": "^1.1.0",
    "@types/express": "^4.0.36",
    "@types/express-serve-static-core": "4.0.49",
    "@types/leaflet": "^1.0.67",
    "@types/memory-cache": "0.0.29",
    "@types/mime": "^1.3.1",
    "@types/node": "^8.0.16",
